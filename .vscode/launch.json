{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost/index.html",
            "webRoot": "${workspaceRoot}/app/files"
        },
        {
            "name": "Launch",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/index.html",
            "port": 9223,
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}",
            "userDataDir": "${workspaceRoot}/out/chrome"
        }
    ]
}